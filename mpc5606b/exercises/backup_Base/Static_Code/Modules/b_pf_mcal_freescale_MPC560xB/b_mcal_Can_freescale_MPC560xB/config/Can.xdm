                        </a:da>
                    </v:var>

                    <v:var name="CanCodeSizeOptimization" type="BOOLEAN">
                        <a:a name="DESC">
                            <a:v>
                                <![CDATA[EN:<html>
                                    Enables optimization of Interrupt Service Routines for code size.<br>
                                    <h1>Note</h1>Implementation specific Parameter. Not AutoSar Required.<br>
                                </html>]]>
                            </a:v>
                        </a:a>
                        <a:a name="LABEL" value="Can Interrupts Size Optimization"/>
