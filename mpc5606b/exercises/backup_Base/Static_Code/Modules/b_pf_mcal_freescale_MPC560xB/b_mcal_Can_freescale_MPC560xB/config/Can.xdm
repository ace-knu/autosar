  <d:ctr type="AUTOSAR" factory="autosar" 
         xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd" 
         xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd" 
         xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd" >
    <d:lst type="TOP-LEVEL-PACKAGES">
      <d:ctr name="TS_T2D13M10I1R0" type="AR-PACKAGE">
        <a:a name="UUID" value="ECUC:181f65b6-f772-4dd7-b9a6-1186c4c7eba8"/>
        <d:lst type="ELEMENTS">
          <d:chc name="Can" type="AR-ELEMENT" value="MODULE-DEF">
            <v:ctr type="MODULE-DEF">
              <a:a name="RELEASE" value="asc:4.0"/>
              <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                <ad:ADMIN-DATA>
                  <ad:DOC-REVISIONS>
                    <ad:DOC-REVISION>
                      <ad:REVISION-LABEL>4.1.0</ad:REVISION-LABEL>
                      <ad:ISSUED-BY>AUTOSAR</ad:ISSUED-BY>
                      <ad:DATE>2010-12-03</ad:DATE>
                    </ad:DOC-REVISION>
                  </ad:DOC-REVISIONS>
                </ad:ADMIN-DATA>
              </a:a>
              <a:a name="DESC" 
                   value="EN: This container holds the configuration of a single CAN Driver."/>
              <a:a name="LOWER-MULTIPLICITY" value="1"/>
              <a:a name="UPPER-MULTIPLICITY" value="*"/>
              <a:a name="UUID" value="ECUC:393e7d2f-39a1-4652-a4d8-59d4b5ab140a"/>
              <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                <a:a name="LABEL" value="Config Variant"/>
                <a:a name="UUID" value="ECUC:40b85d0b-6a49-48a9-96e8-58bbab831aa3"/>
                <a:da name="DEFAULT" value="VariantPostBuild"/>
                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                <a:da name="RANGE">
                  <a:v>VariantPostBuild</a:v>
                  <a:v>VariantPreCompile</a:v>
                </a:da>
              </v:var>
              <v:ctr name="NonAutosar" type="IDENTIFIABLE">
                <a:a name="UUID" value="ECUC:79fb0242-eaf3-11e3-a4db-82687f4fc15c"/>
                <a:a name="DESC">
                    <a:v>
                        <![CDATA[EN:
                            <html>
                                <h1>NonAutosar</h1>
                                <p>Autosar Requirements: </p>
                                <p>This container contains the global configuration parameters of the Non-Autosar Mcu driver. 
                                   This container is a MultipleConfigurationContainer, i.e. this container and 
                                   its sub-containers exist once per configuration set.</p>
                            </html>
                        ]]>
                    </a:v>
                    </a:a>
<!--                <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>-->
                    <v:var name="CanDisableDemReportErrorStatus" type="BOOLEAN">
                        <a:a name="LABEL" value="Can Disable Production Error Reporting"/>
                        <a:a name="DESC">
                        <a:v>
                          <![CDATA[EN:
                              <html>
                                  <h1>CanDisableDemReportErrorStatus</h1>
                                  <p>Switches the Diagnostic Error Reporting and Notification OFF.</p>
                              </html>
                          ]]>
                   </a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                  </a:a>
                 <a:a name="ORIGIN" value="Freescale"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="ECUC:92125f56-eaf3-11e3-a4db-82687f4fc15c"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                    
                <v:var name="CanApiErrorStatus" type="BOOLEAN">
                    <a:a name="DESC">
                        <a:v>
                            <![CDATA[<html>
                                When this parameter is false it Enables support for Can_ErrorStatus() function.
                                Default it is True.
                                <h1>Note</h1>Implementation Specific parameter. Not AutoSar Required.<br>
                            </html>]]>
                        </a:v>
                    </a:a>
                    <a:a name="LABEL" value="CanApiErrorStatus"/>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Freescale"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:754520ea-8aa2-42e5-b937-7627c3967414"/>
                    <a:da name="DEFAULT" value="false"/>
                </v:var>
                    
                    <v:var name="CanApiCheckTxCancel" type="BOOLEAN">
                        <a:a name="DESC">
                            <a:v>
                                <![CDATA[<html>
                                    When this parameter is false it Enables support for Can_CheckTxCancel() function.
                                    Default it is True.
                                    <h1>Note</h1>Implementation Specific parameter. Not AutoSar Required.<br>
                                </html>]]>
                            </a:v>
                        </a:a>
                        <a:a name="LABEL" value="CanApiCheckTxCancel"/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                            <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="Freescale"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:754521ea-8aa2-42e5-b947-7627c3867414"/>
                        <a:da name="DEFAULT" value="false"/>
                    </v:var>
                    

              </v:ctr>
                <v:lst name="CanConfigSet" type="MULTIPLE-CONFIGURATION-CONTAINER">
                    <a:da name="MIN" value="1"/>
                    <a:da name="INVALID" type="XPath">
                        <a:tst expr="(../IMPLEMENTATION_CONFIG_VARIANT = 'VariantPreCompile') and 
                                     (num:i(count(../CanConfigSet/*)) &gt; 1)" 
                                true="For VariantPreCompile, there should be only one CanConfigSet."/>
                    </a:da>

                    <!-- @implements DCAN02005 -->
                    <v:ctr name="CanConfigSet" type="MULTIPLE-CONFIGURATION-CONTAINER">
                        <a:a name="DESC">
                            <a:v>
                                <![CDATA[EN:<html>
                                    CAN343_Conf. This is the multiple configuration set container for CAN Driver.<br>
                                </html>]]>
                            </a:v>
                        </a:a>
                        <a:a name="UUID" value="ECUC:8f338c5b-bc57-4a6a-b94d-e7cfacee8f7d"/>
                    <v:ctr name="CanDemEventParameterRefs" type="IDENTIFIABLE">
                    <a:a name="LABEL" value="Can Dem Event Parameter Refs" />
                    <a:a name="DESC">
                      <a:v>
                        <![CDATA[EN:
                        <html>
                          <p>
                             Container for the references to DemEventParameter elements which shall be invoked using the API Dem_ReportErrorStatus API in case the corresponding error occurs. The EventId is taken from the referenced DemEventParameter&apos;s DemEventId value. The standardized errors are provided in the container and can be extended by vendor specific error references.
                          </p>
                        </html>
                        ]]>
                      </a:v>
                    </a:a>
                      <a:a name="OPTIONAL" value="true"/>
                      <a:da name="ENABLE" value="true"/>
                      <a:a name="UUID"
                           value="ECUC:d10b9ced-78c1-4b42-a675-ea6d477ef52f"/>
                        <!-- /** @implements  */ -->
                        <v:ref name="CAN_E_TIMEOUT"
                               type="SYMBOLIC-NAME-REFERENCE">
                          <a:a name="LABEL" value="CAN E_TIMEOUT" />
                            <a:a name="DESC">
                              <a:v>
                                <![CDATA[EN:
                                <html>
                                  <p>
                                     Reference to configured DEM event to report &quot;Timeout expired&quot;.
                                  </p>
                                </html>
                                ]]>
                              </a:v>
                               </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS"
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PreCompile">VariantPostBuild</icc:v>
                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="Freescale"/>
                          <a:a name="UUID"
                               value="ECUC:8ce16873-adbc-43aa-a3cb-1f3fcc49eb63"/>
                          <a:a name="OPTIONAL" value="true"/>
                          <a:da name="ENABLE" value="true"/>
                          <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Dem/DemConfigSet/DemEventParameter"/>
                        </v:ref>
                    </v:ctr>

                        <v:lst name="CanController" type="MAP">
                            <a:da name="MIN" value="1"/>
                            <a:da name="INVALID" type="XPath">
                                <a:tst expr="num:i(count(node:current()/*)) &gt; ecu:get('Can.CanConfigSet.CanController')" 
                                            true="Maximum CAN Controllers available for the selected derivative was exceeded."/>
                            </a:da>

                            <!-- @implements DCAN02006 -->
                            <v:ctr name="CanController" type="IDENTIFIABLE">
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:<html>
                                            CAN354_Conf. This container contains the configuration parameters of the CAN controller(s).<br>
                                        </html>]]>
                                    </a:v>
                                </a:a>
                                <a:a name="UUID" value="ECUC:2b6f2e90-e380-4a78-bfc4-6ae6631cf1d6"/>

                                <v:var name="CanHwChannel" type="ENUMERATION">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Specifies which one of the on-chip FlexCAN interfaces is associated with this controller ID.<br>
                                                <h1>Note</h1>Implementation Specific parameter. Not AutoSar Required.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Hardware Channel"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:e4482b90-209f-4bbb-bd65-a7ca3b21a48c"/>
                                    <a:da name="DEFAULT" type="XPath" 
                                        expr="(ecu:list('Can.CanConfigSet.CanHwChannelList.MSCAN'))[position()-1=node:fallback(node:current()/../@index,'0')]">
                                    </a:da>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="text:uniq(../../*/CanHwChannel, .)" false="Duplicate physical CAN channel"/>
                                    </a:da>
                                    <a:da name="RANGE" type="XPath" expr="ecu:list('Can.CanConfigSet.CanHwChannelList.MSCAN')"/>
                                </v:var>

                                <!-- @implements DCAN02017 -->
                                <v:var name="CanControllerActivation" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN315_Conf. Defines if a CAN controller is used in the configuration.<br>
                                                Deactivation of a particular CAN controller is equivalent to a CAN controller not used in the configuration.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Controller Activation"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:06986fce-c0d5-402d-b7bd-1ffbb5f2d3e9"/>
                                    <a:da name="DEFAULT" value="true"/>
                                </v:var>

                                <!-- @implements DCAN02007 -->
                                <v:var name="CanControllerBaseAddress" type="INTEGER">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN382_Conf. Specifies the CAN controller base address. <br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Controller Base Address"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:7e05d78c-5cc5-497b-9e7e-3a2d2b8af107"/>
                                    <a:da name="DEFAULT" value="0"/>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&gt;=0"/>
                                        <a:tst expr="&lt;=4294967295"/>
                                    </a:da>
                                    <a:da name="EDITABLE" value="false" />
                                </v:var>
                                
                                <!-- @implements DCAN02019 -->
                                <v:var name="CanControllerId" type="INTEGER">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN316_Conf: This parameter provides the controller ID which is unique in a given CAN Driver.<br>
                                                The value for this parameter starts with 0 and continue without any gaps.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Controller ID"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                    <a:a name="UUID" value="ECUC:18522ce9-9ce2-41d0-a0db-b0a4cf879264"/>
                                    <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index, num:i(1))"/>
                                    <a:da name="RANGE" type="XPath">
                                        <a:tst expr="(node:fallback(.,1) &gt;= 0) and (node:fallback(.,1) &lt; num:i(count(node:fallback(node:current()/../../*, 1 ))))" false="Value out of range: must be in range 0 to N-1 (N is number of configured channels). Use the Calc button to calculate correct default value."/>
                                        <a:tst expr="text:uniq(node:fallback(../../*/CanControllerId, text:split('1 2 3')), node:fallback(.,1) )" false="Duplicated value, CanControllerId must be unique across all Can controllers. Use the Calc button to calculate correct default value."/>                                                 
                                    </a:da>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&gt;=0"/>
                                        <a:tst expr="&lt;=255"/>
                                    </a:da>
                                </v:var>

                                <!-- @implements DCAN02047 -->
                                <v:var name="CanRxProcessing" type="ENUMERATION">
                                    <a:a name="CALCULATION-FORMULA" value="Mode of event triggering: interrupt or polling."/>
                                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                                    <a:a name="DERIVED" value="TRUE"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN317_Conf. Enables/Disables API Can_MainFunction_Read() for handling PDU reception events in polling mode.
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Rx Processing Type"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:f55d547d-37c7-4393-b930-c3cb7b7678b2"/>
                                    <a:da name="DEFAULT" value="Polling"/>
                                    <a:da name="RANGE">
                                        <a:v>Interrupt</a:v>
                                        <a:v>Polling</a:v>
                                    </a:da>
                                </v:var>

                                <!-- @implements DCAN02049 -->
                                <v:var name="CanTxProcessing" type="ENUMERATION">
                                    <a:a name="CALCULATION-FORMULA" value="Mode of event triggering: interrupt or polling."/>
                                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                                    <a:a name="DERIVED" value="TRUE"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN318_Conf. Enables/Disables API Can_MainFunction_Write() for handling PDU transmission events in polling mode
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Tx Processing Type"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:7d955c0a-ac24-4c77-96c8-145fd72fa124"/>
                                    <a:da name="DEFAULT" value="Polling"/>
                                    <a:da name="RANGE">
                                        <a:v>Interrupt</a:v>
                                        <a:v>Polling</a:v>
                                    </a:da>
                                </v:var>

                                <!-- @implements DCAN02016 -->
                                <v:var name="CanBusoffProcessing" type="ENUMERATION">
                                    <a:a name="CALCULATION-FORMULA" value="Mode of event triggering: interrupt or polling."/>
                                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                                    <a:a name="DERIVED" value="TRUE"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN314_Conf. Enables/Disables API Can_MainFunction_BusOff() for handling busoff events in polling mode.
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can BusOff Processing Type"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:0db6e1d2-2097-4965-9197-0fb6928ea144"/>
                                    <a:da name="DEFAULT" value="Polling"/>
                                    <a:da name="RANGE">
                                        <a:v>Interrupt</a:v>
                                        <a:v>Polling</a:v>
                                    </a:da>
                                </v:var>

                                <!-- @implements DCAN02052 -->
                                <v:var name="CanWakeupProcessing" type="ENUMERATION">
                                    <a:a name="CALCULATION-FORMULA" value="Mode of event triggering: interrupt or polling."/>
                                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                                    <a:a name="DERIVED" value="TRUE"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN319_Conf. Enables/Disables API Can_MainFunction_Wakeup() for handling wakeup events in polling mode.<br><br>
                                                <h1>Note</h1>This option is enabled only if global parameter <CanController/CanWakeupsupport> is 'true'.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Wakeup Processing Type"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:cd2c7e0d-b5a9-4b48-ac4f-b62a2f9fc05a"/>
                                    <a:da name="DEFAULT" value="Polling"/>
                                    <a:da name="EDITABLE" type="XPath">
                                        <a:tst expr="../CanWakeupSupport = 'true'"/>
                                    </a:da>
                                    <a:da name="RANGE">
                                        <a:v>Interrupt</a:v>
                                        <a:v>Polling</a:v>
                                    </a:da>
                                </v:var>

                                <v:var name="CanBccSupport" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Defines if Backwards Compatibility Configuration (BCC) feature of CAN controller is used in the configuration.<br>
                                                If BCC feature of CAN controller is enabled, Individual Rx masking and queue feature are disabled.<br>
                                                If BCC feature of CAN controller is disabled, Individual Rx masking and queue feature are enabled.<br>
                                                <h1>Note</h1>Implementation Specific parameter.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can BCC Enable"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:068d5224-99e9-4b35-8f97-7d00d79f5eed"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="INVALID" type="XPath">
                                       <a:tst expr=".='false' and ecu:get('Can.CanConfig.RxIndividualMaskSupport')='STD_OFF'" 
                                          true="This derivative does not support individual masking per MB."/>
                                    </a:da>
                                </v:var>

                                <!-- @implements DCAN02053 -->
                                <v:var name="CanWakeupSupport" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN330_Conf. CAN driver support for wakeup over CAN Bus.<br>
                                                Every WakeUp process will be ignore if this checkbox is not set to ON.<br>
                                                This parameter enables Internal Wakeup (using controller registers) and External Wakeup (using WKUP module).<br>
                                                This is enabled only if internal Wakeup is supported by the platform.
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Wakeup Support"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:8ca7640d-7a8e-4ee5-95d9-55530002ef55"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="EDITABLE" type="XPath">
                                        <a:tst expr="ecu:get('Can.CanConfig.InternalWakeupSupport')='STD_ON'" />
                                    </a:da>
                                </v:var>
                    
                                <v:var name="CanListenOnlyMode" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Enables the Listen Only Mode (CTRL[LOM] bit).<br>
                                                In this mode, transmission is disabled, all error counters are frozen and the module operates in a CAN Error Passive mode.<br>
                                                <h1>Note</h1>Implementation specific Parameter. Not AutoSar Required.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Listen Only Mode (CTRL[LOM])"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:584b7361-c0f1-44a9-b0b7-ab0c057dc7d7"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>

                                <v:var name="CanLoopBackMode" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html> 
                                                CTRL[LPB]: Enables CAN to operate in Loop Back Mode.<br>
                                                The bit stream output of the transmitter is fed back internally to the receiver input.<br>
                                                The Rx CAN input pin is ignored and the Tx CAN output goes to the recessive state (logic "1").<br>
                                                <h1>Note</h1>Implementation specific Parameter. Not AutoSar Required.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Loop Back Mode (CTRL[LPB])"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:64911420-9960-4cf1-99c3-f27e9587d30f"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>

                                <v:var name="CanSoftwareBusOffRecovery" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Enables Software Bus Off recovery when automatic recovering from BusOff state is disabled for CAN controller.<br>
                                                <h1>Note</h1>Implementation specific Parameter. This parameter is enabled when CanAutoBusOffRecovery is set to false.
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Software BusOff Recovery"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:eea96cc8-00c9-4970-b8e6-e6d888520428"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="ENABLE" type="XPath">
                                        <a:tst expr="../CanAutoBusOffRecovery = 'false'"/>
                                    </a:da>
                                </v:var>

                                <!-- @implements DCAN02004 -->
                                <v:var name="CanAutoBusOffRecovery" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Enable/Disable automatic BusOff recovery (CTRL[BOFF_REC] bit).<br>
                                                0(Unchecked) = Automatic recovering from Bus Off state occurs according to the CAN Specification 2.0B.<br> 
                                                1(Checked) = Automatic recovering from Bus Off is disabled and the module remains in Bus Off state until the bit is negated(zero) by the user.<br>
                                                <h1>Note</h1>Implementation specific Parameter. Not AutoSar Required.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Auto BusOff Recovery (CTRL[BOFF_REC])"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:bccd3fb0-98e3-4258-b713-541d67cede83"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>

                                <v:var name="CanTrippleSamplingEnable" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html> 
                                                CTRL[SMP]: This bit defines the sampling mode of CAN bits at the Rx input.<br>
                                                1 - Enables acquisition of 3 samples and majority voting for the value of received bit.<br>
                                                0 - Just one sample is used to determine the bit value.<br>
                                                <h1>Note</h1>Implementation specific Parameter. Not AutoSar Required.<br>
                                             </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Triple Sampling Mode Enable (CTRL[SMP])"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:4863d7f6-a95e-41ba-ac92-b016d82d0be5"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>

                                <v:var name="CanWakeUpSourceFilter" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Defines whether the integrated low-pass filter is applied to protect the Rx CAN input from spurious wake up.<br>
                                                1 = FlexCAN uses the filtered Rx input to detect recessive to dominant edges on the CAN bus.<br>
                                                0 = FlexCAN uses the unfiltered Rx input to detect recessive to dominant edges on the CAN bus.<br>
                                                This option will provide the setting for init the controller with MCR[WAK_SRC] bit set.<br>
                                                <h1>Note</h1>Implementation specific Parameter. Not AutoSar Required.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can WakeUp Source Filter (MCR[WAK_SRC])"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="VISIBLE" type="XPath">
                                        <a:tst expr="ecu:get('Can.CanConfig.CanController.WakeUpSourceFilter')='STD_ON'" />
                                    </a:a>
                                    <a:da name="EDITABLE" type="XPath">
                                        <a:tst expr="ecu:get('Can.CanConfig.CanController.WakeUpSourceFilter')='STD_ON'" />
                                    </a:da>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:18ae2f15-3091-4a50-98eb-2165d32f30a1"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>

                                <v:var name="CanLowestBuffTransmitFirst" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CTRL[LBUF]. This bit defines the ordering mechanism for Message Buffer transmission.<br> 
                                                When asserted, the MCR[LPRIO_EN] bit doesn't affect the priority arbitration.<br>
                                                <h1>Note</h1>Implementation specific Parameter. Not AutoSar Required.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Lowest Buffer Transmit First (CTRL[LBUF])"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:91bce8e7-2677-48df-81f5-523b8492c1b4"/>
                                    <a:da name="DEFAULT" value="true"/>
                                </v:var>

                                <v:var name="CanLocalPriorityEn" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                MCR[LPRIO_EN]. This bit controls whether the local priority feature is enabled or not.<br> 
                                                <h1>Note</h1>Implementation specific Parameter. Not AutoSar Required.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Local Priority Enable (MCR[LPRIO_EN])"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:73d70c23-ca02-4248-a5b3-5d81a68565d4"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>

                                <v:var name="CanWarningEnable" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                MCR[WRN_EN]. Warning Interrupt Enable<br>
                                                When asserted, this bit enables the generation of the TWRN_INT and RWRN_INT flags in the Error and
                                                Status Register. If WRN_EN is negated, the TWRN_INT and RWRN_INT flags will always be zero,
                                                independent of the values of the error counters, and no warning interrupt will ever be generated. This bit must
                                                be written in Freeze mode only.<br>
                                                <h1>Note</h1>Implementation specific Parameter. Not AutoSar Required.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Warning Int Enable (MCR[WRN_EN])"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:d58e9719-4145-4c32-9c21-041e7796e669"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>

                                <v:var name="CanClockFromBus" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Switches the source clock for the module to the system bus (rather than crystal).<br>
                                                1 = The CAN engine clock source is the bus clock.(from MCU)<br>
                                                0 = The CAN engine clock source is the oscillator clock.<br><br>
                                                <h1>Note</h1>Implementation specific Parameter. Not AutoSar Required.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Clock from Bus (CTRL[CLK_SRC])"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:f50b30de-e6b2-4035-b8e2-645a8fbc6a35"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                               
                               <!-- @implements DCAN02900 -->
                           <v:ref name="CanControllerDefaultBaudrate" type="REFERENCE">
                               <a:a name="DESC">
                                   <a:v>
                                       <![CDATA[EN:<html>
                                           CAN435_Conf. Reference to baudrate configuration container configured for the Can Controller.<br>
                                         </html>]]>
                                       </a:v>
                                       </a:a>
                                       <a:a name="LABEL" value="Can Controller Default Baudrate"/>
                                       <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                           <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                           <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                       </a:a>
                                       <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                       <a:a name="UUID" value="ECUC:9171c656-7a2a-4262-833a-a95e19a91f22"/>
                                       <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Can/CanConfigSet/CanController/CanControllerBaudrateConfig"/>
                                       <a:da name="INVALID" type="XPath" expr="node:refvalid(.)" false="The configured node does not exist or may not be referenced.Please select the default baudrate from CanControllerBaudrateConfig container"/>
                                       </v:ref>
            

                                <!-- @implements DCAN02024 -->
                                <v:ref name="CanCpuClockRef" type="REFERENCE">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN313_Conf. Reference to the CPU clock configuration, which is set in the MCU driver configuration.<br>
                                                This control is used only if "CanClockFromBus" = "true".<br>
                                                MCU plugin need to be added and then give the reference to it.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can CPU Reference Clock"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="UUID" value="ECUC:b051dd0a-d2c6-4feb-b096-75f6bfac2fc6"/>
                                    <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Mcu/McuModuleConfiguration/McuClockSettingConfig/McuClockReferencePoint"/>
                                    <a:da name="ENABLE" type="XPath">
                                        <a:tst expr="../CanClockFromBus = 'true'"/>
                                    </a:da>
                                    <a:da name="INVALID" type="XPath" expr="node:refvalid(.)" false="The configured node does not exist or may not be referenced."/>
                                </v:ref>

                                <v:ref name="CanCpuClockRef_Alternate" type="REFERENCE">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Reference to the CPU clock configuration, which is set in the MCU driver configuration.<br>
                                                This control is used only if "CanClockFromBus" = "true".<br>
                                                MCU plugin need to be added and then give the reference to it.<br>
                                                <h1>Note</h1>: This parameter is visible only if Can.CanConfig.DualClockMode=STD_ON from Resource files.<br>
                                                <h1>Note</h1>: This parameter is editable only if CanGeneral/CanEnableDualClockMode is set to "true" and "CanClockFromBus" is set to true.
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can CPU Reference Clock Alternate"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="UUID" value="ECUC:b4351a5e-87ff-4245-b317-a4551797d786"/>
                                    <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Mcu/McuModuleConfiguration/McuClockSettingConfig/McuClockReferencePoint"/>
                                    <a:a name="VISIBLE" type="XPath" expr="ecu:get(&apos;Can.CanConfig.DualClockMode&apos;)='STD_ON'"/>
                                    <a:da name="ENABLE" type="XPath">
                                        <a:tst expr="(../../../../../CanGeneral/CanEnableDualClockMode = 'true') and (../CanClockFromBus = 'true')"/>
                                    </a:da>
                                    <a:da name="INVALID" type="XPath" expr="node:refvalid(.)" false="The configured node does not exist or may not be referenced."/>
                                </v:ref>

                                <v:var name="CanControllerRxFifoEnable" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Defines if RX FIFO feature of CAN controller is used in the configuration.<br>
                                                If FIFO feature of CAN controller is enabled, First 8 Message Buffers will be used by FIFO engine.<br>
                                                <h1>Note</h1>Implementation Specific parameter.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can RxFifo Enable"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:de5ace9c-b301-4801-8b90-eb2e32fc12d6"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="INVALID" type="XPath" expr="(.='true' and node:contains(node:refs(./../../../CanHardwareObject/*[./CanObjectType ='RECEIVE']/CanControllerRef),./../.)) or .='false'" false="Atleast one HRH should be configured for this controller(FIFO is enabled)."/>
                                </v:var>

                                <v:var name="CanRxFifoWarningNotification" type="FUNCTION-NAME">
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN:<html>
                                            Set here the name of the handler for RxFifo Warning notification function.
                                            <h1>Note</h1>Implementation Specific parameter</h1></html>
                                            ]]>
                                        </a:v>
                                    </a:a>                             
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:ea61b545-662c-4516-90e3-55da60b3656c"/>
                                    <a:da name="ENABLE" type="XPath">
                                        <a:tst expr="(../CanControllerRxFifoEnable ='true')"/>
                                    </a:da>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="((normalize-space(.)='NULL') or (normalize-space(.)='Null') or (normalize-space(.)='false') or (normalize-space(.)='FALSE') or (normalize-space(.)='null') or (normalize-space(.)='null_ptr') or (normalize-space(.)='Null_Ptr'))" 
                                            true="Invalid name of the CanRxFifoWarningNotification. Must be valid C function name or NULL_PTR."/>                                                     
                                        <a:tst expr="text:match(normalize-space(.),'^[_a-zA-Z]+[_0-9a-zA-Z]*$')" 
                                            false="Invalid name of the CanRxFifoWarningNotification. Must be valid C function name or NULL_PTR."/>                    
                                        <a:tst expr="((normalize-space(.) = normalize-space(../CanRxFifoOverflowNotification))  and  (normalize-space(.) != 'NULL_PTR'))" 
                                            true="Function name for CanRxFifoOverflowNotification and CanRxFifoWarningNotification should be unique"/>
                                    </a:da>
                                    <a:da name="DEFAULT" value="NULL_PTR"/>
                                </v:var>

                                <v:var name="CanRxFifoOverflowNotification" type="FUNCTION-NAME">
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN:
                                            <html>
                                            Set here the name of the handler for RxFifo Overflow notification function.
                                            <h1>Note</h1>Implementation Specific parameter</h1></html>
                                            ]]>
                                        </a:v>
                                    </a:a>       
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:066a8fed-aa3a-4566-ae25-32cfe7990ed2"/>
                                    <a:da name="ENABLE" type="XPath">
                                        <a:tst expr="(../CanControllerRxFifoEnable ='true')"/>
                                    </a:da>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="((normalize-space(.)='NULL') or (normalize-space(.)='Null') or (normalize-space(.)='false') or (normalize-space(.)='FALSE') or (normalize-space(.)='null') or (normalize-space(.)='null_ptr') or (normalize-space(.)='Null_Ptr'))" 
                                            true="Invalid name of the CanRxFifoOverflowNotification. Must be valid C function name or NULL_PTR."/>                                                     
                                        <a:tst expr="text:match(normalize-space(.),'^[_a-zA-Z]+[_0-9a-zA-Z]*$')" 
                                            false="Invalid name of the CanRxFifoOverflowNotification. Must be valid C function name or NULL_PTR."/>                    
                                    </a:da>
                                    <a:da name="DEFAULT" value="NULL_PTR"/>
                                </v:var>

                                <v:var name="CanErrorControllerNotifEn" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Enables/Disables the Error Controller Notification. If Disabled, no error interrupt or notification shall take place.
                                                <h1>Note</h1>Implementation Specific parameter. Not AutoSar Required.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Error Notification Enable"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:45d2272d-76d5-4d4d-b114-5b81de297f2c"/>
                                    <a:da name="DEFAULT" value="true"/>
                                </v:var>
                                        
                                <v:var name="CanErrorControllerNotification" type="FUNCTION-NAME">
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN:
                                            <html>
                                            Set here the name of the Error Interrupt controller notification function.
                                            <h1>Note</h1>Implementation Specific parameter</h1></html>
                                            ]]>
                                        </a:v>
                                    </a:a>       
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:8050abe2-2e8d-472f-badb-b5a460f53af8"/>
                                    <a:da name="EDITABLE" type="XPath">
                                        <a:tst expr="../CanErrorControllerNotifEn = 'true'"/>
                                    </a:da>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="((normalize-space(.)='NULL') or (normalize-space(.)='Null') or (normalize-space(.)='false') or (normalize-space(.)='FALSE') or (normalize-space(.)='null') or (normalize-space(.)='null_ptr') or (normalize-space(.)='Null_Ptr'))" 
                                            true="Invalid name of the CanErrorControllerNotification. Must be valid C function name or NULL_PTR."/>                                                     
                                        <a:tst expr="text:match(normalize-space(.),'^[_a-zA-Z]+[_0-9a-zA-Z]*$')" 
                                            false="Invalid name of the CanErrorControllerNotification. Must be valid C function name or NULL_PTR."/>                    
                                    </a:da>                    
                                    <a:da name="DEFAULT" value="NULL_PTR"/>
                                </v:var>

                                <!-- @implements DCAN02015 -->
                                <v:ref name="CanWakeupSourceRef" type="SYMBOLIC-NAME-REFERENCE">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN359_Conf. This parameter contains a reference to the Wakeup Source for this controller as defined in the ECU State Manager.<br>
                                                Type: reference to EcuM_WakeupSourceType<br>
                                                EcuM plugin need to be added and then give the reference to it.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:da name="ENABLE" value="false"/>
                                    <a:da name="EDITABLE" type="XPath">
                                        <a:tst expr="../CanWakeupSupport = 'true'"/>
                                    </a:da>
                                    <a:a name="OPTIONAL" value="true"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="UUID" value="ECUC:6911183d-488d-4301-a720-4f9bdae57496"/>
                                    <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuM/EcuMConfiguration/EcuMCommonConfiguration/EcuMWakeupSource"/>
                                </v:ref>

                                <!-- @implements DCAN02008 -->
                                <v:lst name="CanControllerBaudrateConfig" type="MAP">
                                    <a:da name="MIN" value="1"/>
                                    <v:ctr name="CanControllerBaudrateConfig" type="IDENTIFIABLE">
                                        <a:a name="DESC" value="EN: CAN351. This container contains the configuration (parameters) of the CAN Filter Mask(s)."/>
                                        <a:a name="UUID" value="ECUC:bccf9928-1567-49a4-b767-a40100af5bcf"/>
                                        
                                        <v:var name="CanControllerCheckCanStandard" type="BOOLEAN">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        <h1>Note</h1>
                                                        <p>EN: If enabled, Can Plugin checks that CanControllerPropSeg, CanControllerSeg1, CanControllerSeg2 and CanControllerSyncJumpWidth settings
                                                        match the CAN Standard Compliant Bit Time Segment Settings.
                                                        </p>
                                                        <p>See the following table for details:</p>
                                                            <table>
                                                                <tr>
                                                                <th>CanControllerPropSeg + CanControllerSeg1</th>
                                                                <th>CanControllerSeg2</th>
                                                                <th>CanControllerSyncJumpWidth</th>
                                                                </tr>
                                                                <tr> <td>[5 .. 10]</td> <td>2</td> <td>[1 .. 2]</td> </tr>
                                                                <tr> <td>[4 .. 11]</td> <td>3</td> <td>[1 .. 3]</td> </tr>
                                                                <tr> <td>[5 .. 12]</td> <td>4</td> <td>[1 .. 4]</td> </tr>
                                                                <tr> <td>[6 .. 13]</td> <td>5</td> <td>[1 .. 4]</td> </tr>
                                                                <tr> <td>[7 .. 14]</td> <td>6</td> <td>[1 .. 4]</td> </tr>
                                                                <tr> <td>[8 .. 15]</td> <td>7</td> <td>[1 .. 4]</td> </tr>
                                                                <tr> <td>[9 .. 16]</td> <td>8</td> <td>[1 .. 4]</td> </tr>
                                                            </table>
                                                        <h1>Note</h1>Implementation Specific parameter. Not AutoSar Required.<br>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="LABEL" value="Can Time Segments Checking"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:64e64ff3-6427-4cb0-a3f4-b6c1af05ba2a"/>
                                            <a:da name="DEFAULT" value="true"/>
                                        </v:var>
                                        
                                        <v:var name="CanAdvancedSetting" type="BOOLEAN">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        <br>If TRUE initiates the derivation of the CAN bit timing values from the CanControllerBaudRate parameter.<br> 
                                                        When this option is True the CanControllerPropSeg, CanControllerSeg1, CanControllerSeg2, CanControllerSyncJumpWidth are disabled because are background calculated.<br>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="LABEL" value="Can Automatic Time Segments Calculation"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:62203346-f039-4f35-8309-0bdba6a0a555"/>
                                            <a:da name="DEFAULT" value="false"/>
                                        </v:var>

                                        <v:var name="CanBusLength" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        Specifies the CAN Bus length in meters.<br>
                                                        This parameter is used for PROPSEG parameter calculation when "CanAdvancedSetting" control is set to true.<br>
                                                        <h1>Note</h1>Implementation specific Parameter.</h1>
                                                   </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="LABEL" value="Can Bus Length (meters)"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:2e9c3e9c-ff1a-4810-a2b4-92e8220d322b"/>
                                            <a:da name="DEFAULT" value="40"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&gt;=1"/>
                                                <a:tst expr="&lt;=5000"/>
                                            </a:da>
                                            <a:da name="EDITABLE" type="XPath">
                                                <a:tst expr="../CanAdvancedSetting = 'true'"/>
                                            </a:da>
                                        </v:var>

                                        <v:var name="CanPropDelayTranceiver" type="FLOAT">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        Propogation delay of Tranceiver used in nanoseconds.
                                                        <h1>Note</h1>Implementation specific Parameter.<br>
                                                        The calculation for the CAN bit timing is implemented in the code template.<br>
                                                        The Formulas used in the code template for calculation are as follows.<br>
                                                        Physical delay of bus = Bus length * Bus propagation delay.<br>
                                                        tPROP_SEG = 2(Physical delay of bus +CanPropDelayTranceiver ).<br>
                                                        PROP_SEG = ROUND_UP (tPROP_SEG/Bus propagation delay).<br>
                                                        Based on these calculations implemented in the Code template the consistency check is maintained for CanPropDelayTranceiver parameter.<br>
                                                        The PROP_SEG parameter need to be a integral value and not fractional value.</h1>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="LABEL" value="Can Propagation Delay Tranceiver (ns)"/>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:8495da78-1d7c-463f-a081-9af6153bced6"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&gt;=0"/>
                                                <a:tst expr="&lt;=5000"/>
                                            </a:da>
                                            <a:da name="DEFAULT" value="150"/>
                                            <a:da name="EDITABLE" type="XPath">
                                                <a:tst expr="../CanAdvancedSetting = 'true'"/>
                                            </a:da>
                                        </v:var>
              
                                        <v:var name="CanControllerPrescaller" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        Specifies the prescaller for the controller .<br>
                                                        The calculation of the resulting CanControllerTimeQuanta value depending on module clocking and prescaller shall be done offline.<br>
                                                        Prescaler = FreqCanClk / FreqTq; FreqTq = 1 / CanControllerTimeQuanta .<br>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="LABEL" value="Can Controller Prescaller"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:a72da830-6439-46e6-81c8-6c4fd6a65c00"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&gt;=1"/>
                                                <a:tst expr="&lt;=256"/>
                                            </a:da>
                                            <a:da name="DEFAULT" value="100"/>
                                        </v:var>

                                        <v:var name="CanControllerPrescaller_Alt" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        Specifies the alternate prescaller for the controller .<br>
                                                        The calculation of the resulting CanControllerTimeQuanta_Alternate value depending on module clocking and prescaller shall be done offline.<br>
                                                        Prescaler = FreqCanClk / FreqTq; FreqTq = 1 / CanControllerTimeQuanta .<br>
                                                        <h1>Note</h1>: This parameter is visible only if Can.CanConfig.DualClockMode=STD_ON from Resource files.<br>
                                                        <h1>Note</h1>: This parameter is editable only if CanGeneral/CanEnableDualClockMode is set to "true".
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="LABEL" value="Can Controller Prescaller Alternate"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:5c36abb6-7408-4008-982a-e97b28bec0a2"/>
                                            <a:a name="VISIBLE" type="XPath" expr="ecu:get(&apos;Can.CanConfig.DualClockMode&apos;)='STD_ON'"/>
                                            <a:da name="DEFAULT" value="100"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&gt;=1"/>
                                                <a:tst expr="&lt;=256"/>
                                            </a:da>
                                            <a:da name="EDITABLE" type="XPath">
                                                <a:tst expr="../../../../../../../CanGeneral/CanEnableDualClockMode = 'true'"/>
                                            </a:da>
                                            
                                        </v:var>

                                        <!-- @implements DCAN02018 -->
                                        <v:var name="CanControllerBaudRate" type="INTEGER">
                                            <a:a name="LABEL" value="Can Controller BaudRate (Kbps)"/>
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        CAN005_Conf. Specifies the buadrate of the controller in kbps.<br>
                                                        CAN maximum speed is 1Mbps.<br>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:cde9b6db-6c79-4cca-a995-fee4c779ea9b"/>
                                            <a:da name="DEFAULT" value="20"/>
                                            <a:da name="RANGE" type="XPath">
                                                <a:tst expr="text:uniq(node:fallback(../../*/CanControllerBaudRate, text:split('0')), node:fallback(.,1))" false="Duplicate CAN BaudRate configured. Please select the different CanControllerBaudRate in the CanControllerBaudrateConfig container of the same controller"/>
                                            </a:da>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&gt;=0"/>
                                                <a:tst expr="&lt;=2000"/>
                                            </a:da>
                                        </v:var>

                                        <!-- @implements DCAN02020 -->
                                        <v:var name="CanControllerPropSeg" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        CAN073_Conf. It is used to compensate the physical delay within the CAN network (CTRL[PROPSEG] - 0..7).<br>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="LABEL" value="Can Propagation Segment"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:b4433b82-c990-4a04-915a-f9cca0a5c891"/>
                                            <a:da name="DEFAULT" value="5"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&lt;=7"/>
                                                <a:tst expr="&gt;=0"/>
                                            </a:da>
                                            <a:da name="EDITABLE" type="XPath">
                                                <a:tst expr="../CanAdvancedSetting = 'false'"/>
                                            </a:da>
                                        </v:var>

                                        <!-- @implements DCAN02022 -->
                                        <v:var name="CanControllerSeg1" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        CAN074_Conf. Specifies the Phase Segment 1 in time quantas (CTRL[PSEG1] = 0..7).<br>
                                                        PHASE_BUF_SEG1 = (PSEG1 + 1) * Tq .<br>
                                                        The PHASE_BUF_SEG1 valid values are 1-8 Tq.<br>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="LABEL" value="Can Phase Segment 1"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:3cef4e6f-8203-40c8-97f7-1d9f7ac94e73"/>
                                            <a:da name="DEFAULT" value="5"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&gt;=0"/>
                                                <a:tst expr="&lt;=7"/>
                                            </a:da>
                                            <a:da name="EDITABLE" type="XPath">
                                                <a:tst expr="../CanAdvancedSetting = 'false'"/>
                                            </a:da>
                                        </v:var>

                                        <!-- @implements DCAN02023 -->
                                        <v:var name="CanControllerSeg2" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        CAN075_Conf. Specifies Phase Segment 2 in time quantum (CTRL[PSEG2] = 1..7).<br>
                                                        PHASE_BUF_SEG2 = (PSEG2 + 1) * Tq .<br>
                                                        The PHASE_BUF_SEG2 valid values are 2-8 Tq.<br>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="LABEL" value="Can Phase Segment 2"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:8fc49e8b-beed-4fe1-9fe5-8e5218967d0b"/>
                                            <a:da name="DEFAULT" value="6"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&gt;=1"/>
                                                <a:tst expr="&lt;=7"/>
                                            </a:da>
                                            <a:da name="RANGE" type="XPath">
                                            <a:tst expr="(num:i(num:i(../CanControllerPropSeg) + num:i(../CanControllerSeg1) + num:i(.) + 4) &lt; 8) or (num:i(num:i(../CanControllerPropSeg) + num:i(../CanControllerSeg1) + num:i(.) + 4) &gt; 25)" 
                                                        true="No of CanTimeQuantas should be between [8 .. 25]"/>
                                            </a:da>
                                            <a:da name="EDITABLE" type="XPath">
                                                <a:tst expr="../CanAdvancedSetting = 'false'"/>
                                            </a:da>
                                        </v:var>

                                        <!-- @implements DCAN02009 -->
                                        <v:var name="CanControllerSyncJumpWidth" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        CAN383_Conf.Specifies Synchronization Jump Width: CTRL[RJW] = 0..3.<br>
                                                        <h1>Note</h1>Implementation specific Parameter. Not AutoSar required.<br>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="LABEL" value="Can Resynch Jump Width"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:1762af43-3493-4392-a39a-aaff019a96a5"/>
                                            <a:da name="DEFAULT" value="0"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&gt;=0"/>
                                                <a:tst expr="&lt;=3"/>
                                            </a:da>
                                            <a:da name="EDITABLE" type="XPath">
                                                <a:tst expr="../CanAdvancedSetting = 'false'"/>
                                            </a:da>
                                        </v:var>
                                    </v:ctr>
                                </v:lst>
                                
                                <!-- @implements DCAN02026 -->
                                <v:lst name="CanFilterMask" type="MAP">
                                    <a:da name="MIN" value="1"/>
                                    <a:da name="INVALID" type="XPath">
                                    <a:tst expr="(../CanBccSupport = 'true') and (num:i(count(node:current()/*)) &gt;3)" 
                                          true="Maximum available filter masks for the selected derivative was exceeded. Only three filter masks are allowed.
                                                                Configure CanFilterMask_0 for MB0-MB13 and MB16-MB31(Corresponds to RxGlobalMask),
                                                                Configure CanFilterMask_1 for MB14 (Corresponds to Rx14Mask),
                                                                Configure CanFilterMask_2 for MB15 (Corresponds to Rx15Mask)"/>
                                    </a:da>
                                    <v:ctr name="CanFilterMask" type="IDENTIFIABLE">
                                        <a:a name="DESC" value="EN: CAN351. This container contains the configuration (parameters) of the CAN Filter Mask(s)."/>
                                        <a:a name="UUID" value="ECUC:3e71ce00-f95f-4a2c-b48d-88f3dd812515"/>

                                        <!-- @implements DCAN02028 -->
                                        <v:var name="CanFilterMaskValue" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        CAN066_Conf. Describes a mask for hardware-based filtering of CAN identifiers.<br>
                                                        It shall be distinguished between<br>
                                                          - Standard identifier mask<br>
                                                          - Extended identifier mask<br>
                                                        Check if the Can Message type is standard or mixed/extended and based on this the CanFilterMaskValue need to be within 0 to 0x7FF or 0 to 0x1FFFFFFF respectively.<br>
                                                         0 - the corresponding bit in the filter is "don't care".
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="LABEL" value="Can Filter Mask"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:da name="RANGE" type="XPath">
                                                <a:tst expr="(node:fallback(.,1) &gt; 4294967295) or (node:fallback(.,1) &lt; 0)"
                                                        true="Please select valid filter mask value."/>
                                            </a:da>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&gt;=0"/>
                                                <a:tst expr="&lt;=4294967295"/>
                                            </a:da>
                                            <a:a name="UUID" value="ECUC:c110c6f4-293c-48ff-bddb-4e87fe446cc9"/>
                                            <a:da name="DEFAULT" value="2047"/>
                                        </v:var>
                                    </v:ctr>
                                </v:lst>

                                <v:ctr name="CanRxFifo" type="IDENTIFIABLE">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                This container contains the configuration (parameters) of the CAN RX FIFO.<br>
                                                <h1>Note</h1>This container is ENABLED only when CanControllerRxFifoEnable is TRUE.
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can RxFifo"/>
                                    <a:a name="UUID" value="ECUC:29e32cb6-cba3-44ea-871d-4899c0c4e298"/>
                                    <a:da name="ENABLE" type="XPath"> 
                                        <a:tst expr="../CanControllerRxFifoEnable = 'true'"/>
                                    </a:da>

                                    <v:var name="CanControllerIDAcceptanceMode" type="ENUMERATION">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:<html>
                                                This 2-bit field identifies the format of the elements of the Rx FIFO filter table.  
                                                <h1>Note</h1>Implementation Specific parameter.</h1>
                                                Format             Explanation<br>
     
                                                <h1>A      </h1>:  One full ID (standard or extended) per filter element.</h1><br>
                                                <h1>B      </h1>:  Two full standard IDs or two partial 14-bit extended IDs per filter element.</h1><br>
                                                <h1>C      </h1>:  Four partial 8-bit IDs (standard or extended) per filter element.</h1><br>
                                                <h1>D      </h1>:  All frames rejected. Note: The TableID and Filter Mask values don't care for this option.</h1><br>

                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="LABEL" value="Can RxFifo ID Acceptance Mode"/>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="Freescale"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:3ca64f10-a8a6-48f7-9575-3f1cec8adefa"/>
                                        <a:da name="DEFAULT" value="A"/>
                                        <a:da name="ENABLE" type="XPath">
                                            <a:tst expr="../../CanControllerRxFifoEnable = 'true'"/>
                                        </a:da>
                                        <a:da name="RANGE">
                                            <a:v>A</a:v>
                                            <a:v>B</a:v>
                                            <a:v>C</a:v>
                                            <a:v>D</a:v>
                                        </a:da>
                                    </v:var>

                                    <v:lst name="CanRxFifoTable" type="MAP">
                                        <a:da name="MIN" value="0"/>
                                        <a:da name="INVALID" type="XPath">
                                            <a:tst expr="num:i(count(node:current()/*)) &gt; num:i(8)" true="Maximum CAN FIFO elements available are exceeded."/>
                                        </a:da>
                                        <v:ctr name="CanRxFifoTable" type="IDENTIFIABLE">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:<html>
                                                        This container contains the 8-entry ID table information required to configure RX FIFO.
                                                        <h1>Note</h1>This container is ENABLED only when CanControllerRxFifoEnable is TRUE. Multiplicity is 8-8 to have 8 sub-containers containing configuration parameters for 8 entry FIFO ID table.
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="UUID" value="ECUC:a2e004c3-d5fc-4438-8f2a-dda9656a910c"/>

                                            <v:var name="CanIDValue0" type="INTEGER">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:<html>
                                                            Specifies an ID to be used as acceptance criteria for the ID Table 0.
                                                            <h1>Note</h1>Implementation Specific parameter. Value for this parameter should be enterd based on the CanControllerIDAcceptanceMode selected as explained below.<br>
            
                                                            <h1>A      </h1>:  One full ID (standard or extended) per filter element.<br>
                                                            <h1>B      </h1>:  One full standard ID if the CanTableIDType is Standard or one 14 most significant bit value of Extended ID if the CanTableIDType is Extended.<br>
                                                            <h1>C      </h1>:  One 8 most significant bit value of  Standard or Extended ID.<br>
                                                        </html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="LABEL" value="Can RxFifo Identifier 0"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="ORIGIN" value="Freescale"/>
                                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                <a:a name="UUID" value="ECUC:b5c35d84-fd41-4d36-81ba-66bc04c0ef2d"/>
                                                <a:da name="DEFAULT" value="255"/>
                                                <a:da name="ENABLE" type="XPath">
                                                    <a:tst expr="(node:fallback(../../../../CanControllerRxFifoEnable,'')='true') and 
                                                                 ( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='A') or 
                                                                   (node:fallback(../../../CanControllerIDAcceptanceMode,'')='B') or 
                                                                   (node:fallback(../../../CanControllerIDAcceptanceMode,'')='C')
                                                                 )"/>
                                                </a:da>
                                                <a:a name="RANGE" type="XPath">
                                                <a:tst expr=" ( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='A') or 
                                                                (node:fallback(../../../CanControllerIDAcceptanceMode,'')='B')
                                                              ) and 
                                                              ( (.&lt; 0 or  .&gt; 2047) and
                                                                (node:fallback(../CanTableIDType,'')='Standard')
                                                              )" 
                                                            true="CanIDValue0 is invalid. Please enter within the range 0x0-0x7FF for Standard CanTableIDType type"/>
                                                    <a:tst expr=" (node:fallback(../../../CanControllerIDAcceptanceMode,'')='A') and
                                                                  ( (.&lt; 0 or .&gt; 536870911) and 
                                                                    (node:fallback(../CanTableIDType,'')='Extended')
                                                                  )" 
                                                            true="CanIDValue0 is invalid. Please enter within the range 0x0-0x1FFF.FFFF for Extended CanTableIDType type"/>
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='B') and
                                                                   ( (.&lt; 0 or .&gt; 16383) and
                                                                     (node:fallback(../CanTableIDType,'')='Extended')
                                                                   )
                                                                 )"
                                                            true="CanIDValue0 is invalid. Please enter within the range 0x0-0x3FFF[Format B] for Extended CanTableIDType type"/>
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='C') and 
                                                                   (.&lt; 0 or .&gt; 255)
                                                                 )"
                                                            true="CanIDValue0 is invalid. Please enter within the range 0x0-0xFF[Format C] for Extended CanTableIDType type "/>
                                                </a:a>
                                                <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&gt;=0"/>
                                                    <a:tst expr="&lt;=536870911"/>
                                                </a:da>
                                            </v:var>

                                            <v:var name="CanIDValue1" type="INTEGER">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:<html>
                                                            Specifies an ID to be used as acceptance criteria for the ID Table.<br>
                                                            <h1>Note</h1>Implementation Specific parameter. Value for this parameter should be enterd based on the CanControllerIDAcceptanceMode selected as explained below.<br>
                        
                                                            <h1>B      </h1>:   One full standard ID if the CanTableIDType is Standard or one 14 most significant bit value of Extended ID if the CanTableIDType is Extended.<br>
                                                            <h1>C      </h1>:   One 8 most significant bit value of  Standard or Extended ID.<br>
                                                        </html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="LABEL" value="Can RxFifo Identifier 1"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="ORIGIN" value="Freescale"/>
                                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                <a:a name="UUID" value="ECUC:92f3ff2a-626e-43d2-aca7-f838bf8b5c2b"/>
                                                <a:da name="DEFAULT" value="255"/>
                                                <a:da name="ENABLE" type="XPath">
                                                    <a:tst expr="(node:fallback(../../../../CanControllerRxFifoEnable,'')='true') and 
                                                                 ( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='B') or 
                                                                   (node:fallback(../../../CanControllerIDAcceptanceMode,'')='C')
                                                                 ) "/>
                                                </a:da>
                                                <a:a name="RANGE" type="XPath">
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='B') and
                                                                   ( (.&lt; 0 or .&gt; 2047) and 
                                                                     (node:fallback(../CanTableIDType,'')='Standard')
                                                                   )
                                                                 )" 
                                                            true="CanIDValue1 is invalid. Please enter within the range 0x0-0x7FF for Standard CanTableIDType type"/>
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='B') and 
                                                                   ( (.&lt; 0 or .&gt; 16383) and 
                                                                     (node:fallback(../CanTableIDType,'')='Extended')
                                                                   )
                                                                 )" 
                                                            true="CanIDValue1 is invalid. Please enter within the range 0x0-0x3FFF[Format B] for Extended CanTableIDType type"/>
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='A') and
                                                                   ( (.&lt; 0 or .&gt; 2047) and 
                                                                     (node:fallback(../CanTableIDType,'')='Standard')
                                                                   )
                                                                 )" 
                                                            true="CanIDValue1 is invalid. Please enter within the range 0x0-0x7FF for Standard CanTableIDType type and 0x0-0x1FFF.FFFF for Extended CanTableIDType type"/>
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='A') and 
                                                                   ( (.&lt; 0 or .&gt; 536870911) and 
                                                                     (node:fallback(../CanTableIDType,'')='Extended')
                                                                   )
                                                                 )" 
                                                            true="CanIDValue1 is invalid. Please enter within the range 0x0-0x7FF for Standard CanTableIDType type and 0x0-0x1FFF.FFFF for Extended CanTableIDType type"/>
                                                </a:a>
                                                <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&gt;=0"/>
                                                    <a:tst expr="&lt;=536870911"/>
                                                </a:da>
                                            </v:var>

                                            <v:var name="CanIDValue2" type="INTEGER">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:<html>
                                                            Specifies an ID to be used as acceptance criteria for the ID Table.
                                                            <h1>Note</h1>Implementation Specific parameter. Value for this parameter should be enterd based on the CanControllerIDAcceptanceMode selected as explained below.<br>
                                                            <h1>C  </h1>:            One 8 most significant bit value of  Standard or Extended ID.<br>
                                                        </html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="LABEL" value="Can RxFifo Identifier 2"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="ORIGIN" value="Freescale"/>
                                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                <a:a name="UUID" value="ECUC:8e1663a3-9464-4f67-aea2-9c9117e3ae4d"/>
                                                <a:da name="DEFAULT" value="255"/>
                                                <a:da name="ENABLE" type="XPath">
                                                    <a:tst expr="(node:fallback(../../../../CanControllerRxFifoEnable,'')='true') and 
                                                                 (node:fallback(../../../CanControllerIDAcceptanceMode,'')='C')"/>
                                                </a:da>
                                                <a:a name="RANGE" type="XPath">
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='C') and 
                                                                   (.&lt; 0 or .&gt; 255)
                                                                 )" 
                                                            true="CanIDValue2 is invalid. Please enter within the range 0x0-0xFF[Format C] for Extended CanTableIDType type "/>
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='A') and
                                                                   ( (.&lt; 0 or .&gt; 2047) and 
                                                                     (node:fallback(../CanTableIDType,'')='Standard')
                                                                   )
                                                                 )" 
                                                            true="CanIDValue2 is invalid. Please enter within the range 0x0-0x7FF for Standard CanTableIDType type and 0x0-0x1FFF.FFFF for Extended CanTableIDType type"/>
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='A') and
                                                                   ( (.&lt; 0 or .&gt; 536870911) and 
                                                                     (node:fallback(../CanTableIDType,'')='Extended')
                                                                   )
                                                                 )" 
                                                            true="CanIDValue2 is invalid. Please enter within the range 0x0-0x7FF for Standard CanTableIDType type and 0x0-0x1FFF.FFFF for Extended CanTableIDType type"/>
                                                </a:a>
                                                <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&gt;=0"/>
                                                    <a:tst expr="&lt;=536870911"/>
                                                </a:da>
                                            </v:var>

                                            <v:var name="CanIDValue3" type="INTEGER">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:<html>
                                                            Specifies an ID to be used as acceptance criteria for the ID Table.
                                                            <h1>Note</h1>Implementation Specific parameter. Value for this parameter should be enterd based on the CanControllerIDAcceptanceMode selected as explained below.<br>
                                                            <h1>C  </h1>:            One 8 most significant bit value of  Standard or Extended ID.<br>
                                                        </html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="LABEL" value="Can RxFifo Identifier 3"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="ORIGIN" value="Freescale"/>
                                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                <a:a name="UUID" value="ECUC:6767d326-f7b3-40fa-83cb-483fbd77858b"/>
                                                <a:da name="DEFAULT" value="255"/>
                                                <a:da name="ENABLE" type="XPath">
                                                    <a:tst expr="(node:fallback(../../../../CanControllerRxFifoEnable,'')='true') and 
                                                                 (node:fallback(../../../CanControllerIDAcceptanceMode,'')='C')"/>
                                                </a:da>
                                                <a:a name="RANGE" type="XPath">
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='C') and 
                                                                   (.&lt; 0 or .&gt; 255)
                                                                 )" 
                                                            true="CanIDValue3 is invalid. Please enter within the range 0x0-0xFF[Format C] for Extended CanTableIDType type"/>
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='A') and
                                                                   ( (.&lt; 0 or .&gt; 2047) and 
                                                                     (node:fallback(../CanTableIDType,'') = 'Standard')
                                                                   )
                                                                 )" 
                                                            true="CanIDValue3 is invalid. Please enter within the range 0x0-0x7FF for Standard CanTableIDType type and 0x0-0x1FFF.FFFF for  Extended CanTableIDType type"/>
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='A') and 
                                                                   ( (.&lt; 0 or .&gt; 536870911) and 
                                                                     (node:fallback(../CanTableIDType,'') = 'Extended')
                                                                   )
                                                                 )" 
                                                            true="CanIDValue3 is invalid. Please enter within the range 0x0-0x7FF for Standard CanTableIDType type and 0x0-0x1FFF.FFFF for  Extended CanTableIDType type"/>
                                                </a:a>
                                                <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&gt;=0"/>
                                                    <a:tst expr="&lt;=536870911"/>
                                                </a:da>
                                            </v:var>

                                            <v:var name="CanTableIDType" type="ENUMERATION">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:<html>
                                                            Specifies whether extended or standard frames are accepted into the FIFO.<br>
                                                            <h1>Note</h1>Implementation Specific parameter.
                                                        </html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="LABEL" value="Can RxFifo Frame Accepted Mode"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="ORIGIN" value="Freescale"/>
                                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                <a:a name="UUID" value="ECUC:4af727f0-9f09-4e06-b265-48e740c07378"/>
                                                <a:da name="DEFAULT" value="Standard"/>
                                                <a:da name="ENABLE" type="XPath">
                                                    <a:tst expr="(../../../../CanControllerRxFifoEnable = 'true') and 
                                                                 (../../../CanControllerIDAcceptanceMode != 'C')"/>
                                                </a:da>
                                                <a:da name="RANGE">
                                                    <a:v>Standard</a:v>
                                                    <a:v>Extended</a:v>
                                                </a:da>
                                            </v:var>

                                            <v:var name="CanMBFilterMaskValue" type="INTEGER">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:<html>
                                                            Specifies Filetr Mask value to be used as acceptance criteria for the ID Table.<br>
                                                            <h1>Note</h1>Implementation Specific parameter. Value for this parameter should be entered based on the CanControllerIDAcceptanceMode selected as explained below.<br>
                                                            <h1>A </h1>: Filtermask value for One full standard or Extended ID based on the CanTableIDType selected.<br>
                                                            <h1>B </h1>: Filtermask value for CanIDValue0(bit field : 0-11 for standard ID type and 0-14 for extended ID type) and CanIDValue1 (bit field : 12-22 for standard ID type and 15-28 for extended ID type).<br>
                                                            <h1>C </h1>: Filtermask value for CanIDValue0(bit field : 0-8), CanIDValue1(bit field : 9-16), CanIDValue2(bit field : 17-24) and CanIDValue3(bit field : 25-32).<br>
                                                        </html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="LABEL" value="Can RxFifo Filter Mask"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="ORIGIN" value="Freescale"/>
                                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                <a:a name="UUID" value="ECUC:3eb6933b-964c-4763-9a72-ff58f61f90c5"/>
                                                <a:da name="DEFAULT" value="2047"/>
                                                <a:da name="ENABLE" type="XPath">
                                                    <a:tst expr="node:fallback(../../../../CanControllerRxFifoEnable,'')='true'"/>
                                                </a:da>
                                                <a:da name="RANGE" type="XPath">
                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='A') and 
                                                                   ( (.&lt; 0 or .&gt; 2047) and 
                                                                     (node:fallback(../CanTableIDType,'')='Standard')
                                                                   )
                                                                 )" 
                                                            true="Invalid Value - CanMBFilterMaskValue value for CanTableIDType Standard must be in the range 0 to 2047"/>

                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='A') and 
                                                                   ( (.&lt; 0 or .&gt; 536870911) and 
                                                                     (node:fallback(../CanTableIDType,'')='Extended')
                                                                   )
                                                                 )" 
                                                            true="Invalid Value - CanMBFilterMaskValue value for CanTableIDType Extended must be in the range 0 to 536870911"/>

                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='B') and 
                                                                   ( (.&lt; 0 or .&gt; 268435455) and 
                                                                     (node:fallback(../CanTableIDType,'')='Extended')
                                                                   )
                                                                 )" 
                                                            true="Invalid Value - CanMBFilterMaskValue value for CanTableIDType Extended must be in the range 0 to 268435455"/>

                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='B') and 
                                                                   ( (.&lt; 0 or .&gt; 4194303) and 
                                                                     (node:fallback(../CanTableIDType,'')='Standard')
                                                                   )
                                                                 )" 
                                                            true="Invalid Value - CanMBFilterMaskValue value for CanTableIDType Extended must be in the range 0 to 4194303"/>

                                                    <a:tst expr="( (node:fallback(../../../CanControllerIDAcceptanceMode,'')='C') and 
                                                                   (.&lt; 0 or .&gt; 4294967295)
                                                                 )" 
                                                            true="Invalid Value - CanMBFilterMaskValue value must be in the range 0 to 4294967295"/>
                                                </a:da>
                                                <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=4294967295"/>
                                                    <a:tst expr="&gt;=0"/>
                                                </a:da>
                                            </v:var>

                                        </v:ctr>
                                    </v:lst>
                                </v:ctr>
                                <v:ctr name="CanTTController" type="IDENTIFIABLE">
                                    <a:da name="ENABLE" value="false"/>
                                    <a:da name="EDITABLE" value="false"/>
                                    <a:a name="DESC" 
                                         value="EN: This container is only included and valid if TTCAN SWS is used and TTCAN is enabled."/>
                                    <a:a name="OPTIONAL" value="true"/>
                                    <a:a name="UUID" 
                                         value="ECUC:a9ffd090-0ca8-40b5-bea6-d810478c69c3"/>
                                    <a:da name="ENABLE" value="false"/>
                                    <v:var name="CanTTControllerApplWatchdogLimit" 
                                           type="INTEGER">
                                      <a:a name="DESC" 
                                           value="EN: Defines the maximum time period (unit is 256 times NTU) after which the application has to serve the watchdog."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:9fb2caac-3bf6-439c-a048-218212cc42f7"/>
                                      <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=255"/>
                                        <a:tst expr="&gt;=0"/>
                                      </a:da>
                                    </v:var>
                                    <v:var name="CanTTControllerCycleCountMax" 
                                           type="INTEGER">
                                      <a:a name="DESC" 
                                           value="EN: Defines the value for cycle_count_max."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:2575bebc-bf3a-4f13-82aa-495e4bb8a04b"/>
                                      <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=63"/>
                                        <a:tst expr="&gt;=0"/>
                                      </a:da>
                                    </v:var>
                                    <v:var name="CanTTControllerExpectedTxTrigger" 
                                           type="INTEGER">
                                      <a:a name="DESC" 
                                           value="EN: Number of expected_tx_trigger."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:8856c441-4cec-4573-9cfd-30f250cc432a"/>
                                      <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=255"/>
                                        <a:tst expr="&gt;=0"/>
                                      </a:da>
                                    </v:var>
                                    <v:var 
                                           name="CanTTControllerExternalClockSynchronisation" 
                                           type="BOOLEAN">
                                      <a:a name="DESC" 
                                           value="EN: Enables/disables the external clock synchronization."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:bd92a9db-bf9c-475f-8316-74db792f7a5b"/>
                                    </v:var>
                                    <v:var name="CanTTControllerGlobalTimeFiltering" 
                                           type="BOOLEAN">
                                      <a:a name="DESC" 
                                           value="EN: Enables/disables the global time filtering."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:5ade4f75-a795-4aa2-b153-07925df45b6d"/>
                                    </v:var>
                                    <v:var name="CanTTControllerInitialRefOffset" 
                                           type="INTEGER">
                                      <a:a name="DESC" 
                                           value="EN: Defines the initial value for ref trigger offset."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:13536611-35b0-4048-be59-78e5d5717f5c"/>
                                      <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=127"/>
                                        <a:tst expr="&gt;=0"/>
                                      </a:da>
                                    </v:var>
                                    <v:var name="CanTTControllerInterruptEnable" 
                                           type="INTEGER">
                                      <a:a name="DESC" 
                                           value="EN: Enables/disables the respective interrupts."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:18042557-b2ed-4789-9f0e-aad429ec5630"/>
                                      <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=1023"/>
                                        <a:tst expr="&gt;=0"/>
                                      </a:da>
                                    </v:var>
                                    <v:var name="CanTTControllerLevel2" type="BOOLEAN">
                                      <a:a name="DESC" 
                                           value="EN: Defines whether Level 2 or Level 1 is used."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:971da238-795d-4994-96d6-c720a0b1a95f"/>
                                    </v:var>
                                    <v:var name="CanTTControllerNTUConfig" type="FLOAT">
                                      <a:a name="DESC" 
                                           value="EN: Defines the config value for NTU (network time unit)."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:b2fd6b8e-41a5-4969-b20c-82a20f3694e3"/>
                                      <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=100.0"/>
                                        <a:tst expr="&gt;=0.0"/>
                                      </a:da>
                                    </v:var>
                                    <v:var name="CanTTControllerOperationMode" 
                                           type="ENUMERATION">
                                      <a:a name="DESC" 
                                           value="EN: Defines the operation mode."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:e2970a86-941d-4f15-bba3-2ce87ed3473f"/>
                                      <a:da name="RANGE">
                                        <a:v>CAN_TT_EVENT_SYNC_TIME_TRIGGERED</a:v>
                                        <a:v>CAN_TT_EVENT_TRIGGERED</a:v>
                                        <a:v>CAN_TT_TIME_TRIGGERED</a:v>
                                      </a:da>
                                    </v:var>
                                    <v:var name="CanTTControllerSyncDeviation" type="FLOAT">
                                      <a:a name="DESC" 
                                           value="EN: Defines the maximum synchronization deviation:"/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:bf1255bc-0069-4c0e-8d8e-3dd755128cc5"/>
                                      <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=100.0"/>
                                        <a:tst expr="&gt;=0.0"/>
                                      </a:da>
                                    </v:var>
                                    <v:var name="CanTTControllerTURRestore" type="BOOLEAN">
                                      <a:a name="DESC" 
                                           value="EN: Enables/disables the TUR restore."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:12ba3a9c-15e0-4769-a2bc-bcd06955272c"/>
                                    </v:var>
                                    <v:var name="CanTTControllerTimeMaster" type="BOOLEAN">
                                      <a:a name="DESC" 
                                           value="EN: Defines whether the controller acts as a potential time master."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:14bedb12-6a53-48a7-9ec8-e1696f604deb"/>
                                    </v:var>
                                    <v:var name="CanTTControllerTimeMasterPriority" 
                                           type="INTEGER">
                                      <a:a name="DESC" 
                                           value="EN: Defines the time master priority."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:9d6a1705-51bd-4ca3-a632-b17e36d55417"/>
                                      <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=7"/>
                                        <a:tst expr="&gt;=0"/>
                                      </a:da>
                                    </v:var>
                                    <v:var name="CanTTControllerTxEnableWindowLength" 
                                           type="INTEGER">
                                      <a:a name="DESC" 
                                           value="EN: Length of the tx enable window given in CAN bit times."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:da768c8c-c171-40f8-8659-b6f9e31769d6"/>
                                      <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=16"/>
                                        <a:tst expr="&gt;=1"/>
                                      </a:da>
                                    </v:var>
                                    <v:var name="CanTTControllerWatchTriggerGapTimeMark" 
                                           type="INTEGER">
                                      <a:a name="DESC" 
                                           value="EN: watch trigger time mark after a gap"/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:f5d94e7a-8aad-47ef-9f4b-4e96f21f1f4b"/>
                                      <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=65535"/>
                                        <a:tst expr="&gt;=0"/>
                                      </a:da>
                                    </v:var>
                                    <v:var name="CanTTControllerWatchTriggerTimeMark" 
                                           type="INTEGER">
                                      <a:a name="DESC" value="EN: watch trigger time mark"/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:08e45e12-11d2-48f4-bce5-923ef282615a"/>
                                      <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=65535"/>
                                        <a:tst expr="&gt;=0"/>
                                      </a:da>
                                    </v:var>
                                    <v:var name="CanTTIRQProcessing" type="ENUMERATION">
                                      <a:a name="DESC" 
                                           value="EN: Enables / disables API Can_MainFunction_BusOff() for handling busoff events in polling mode."/>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" 
                                           value="ECUC:bc191f73-b47c-4123-9d6e-b250b4f1a03b"/>
                                      <a:da name="RANGE">
                                        <a:v>INTERRUPT</a:v>
                                        <a:v>POLLING</a:v>
                                      </a:da>
                                    </v:var>
                                  </v:ctr>
                            </v:ctr>
                        </v:lst>

                        <!-- @implements DCAN02034 -->
                        <v:lst name="CanHardwareObject" type="MAP">
                            <a:da name="MIN" value="1"/>
                            <v:ctr name="CanHardwareObject" type="IDENTIFIABLE">
                                <a:a name="DESC" value="EN: SWS 324. This container contains the configuration (parameters) of CAN Hardware Objects."/>
                                <a:a name="UUID" value="ECUC:67072344-3de0-4fe3-be6f-011a76490c75"/>

                                <!-- @implements DCAN02030 -->
                                <v:var name="CanHandleType" type="ENUMERATION">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN323_Conf. Specifies the type (Full-CAN or Basic-CAN) of a hardware object.<br>
                                                This configuration element is used as information for the CAN Interface only.<br>
                                                The relevant CAN driver configuration is done with the filter mask and identifier.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Implementation Type"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:eb0670b6-e848-47ce-9d0b-1723d30be663"/>
                                    <a:da name="DEFAULT" value="BASIC"/>
                                    <a:da name="RANGE">
                                        <a:v>BASIC</a:v>
                                        <a:v>FULL</a:v>
                                    </a:da>
                                </v:var>

                                <!-- @implements DCAN02035 -->
                                <v:var name="CanIdType" type="ENUMERATION">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN065_Conf. Specifies whether the IdValue is of type<br>
                                                    - standard identifier (ID - 11 bits length)<br>
                                                    - extended identifier (ID - 29 bits length)<br>
                                                    - mixed mode (standard or extended)<br>
                                                <h1>Note</h1>MBs configred as MIXED standard and RECEIVE type will be treated as EXTENDED.
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can ID Message Type"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:f0d5f8cb-9b96-4d45-b795-b547fdb56ab5"/>
                                    <a:da name="DEFAULT" value="STANDARD"/>
                                    <a:da name="RANGE">
                                        <a:v>EXTENDED</a:v>
                                        <a:v>MIXED</a:v>
                                        <a:v>STANDARD</a:v>
                                    </a:da>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(. = 'MIXED') and (../../../../../CanGeneral/CanExtendedIDSupport = 'false')" true="CanExtendedIDSupport must be enabled for IDs of 29 bits."/>
                                        <a:tst expr="(. = 'EXTENDED') and (../../../../../CanGeneral/CanExtendedIDSupport = 'false')" true="CanExtendedIDSupport must be enabled for IDs of 29 bits."/>                                                    
                                    </a:da>
                                </v:var>

                                <!-- @implements DCAN02036 -->
                                <v:var name="CanIdValue" type="INTEGER">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN325_Conf. Specifies (together with the filter mask)- the identifiers range that passes the hardware filter for of RX objects.<br>
                                                Parameter ranges from 0 to 0x7FF (11 bits) for Standard IDs and 0 to 0x1FFFFFFF (29 bits) for Extended IDs.<br>
                                                User can assign any code to this parameter, but must to respect the above rule related to Standard/Extended IDs.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can ID Message Value"/>
                                    <a:a name="LABEL" value="CanIdValue (Message ID)"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:8aeb59a0-dee5-4286-b2e4-564872de6bdb"/>
                                    <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index,'0')"/>
                                    <a:da name="RANGE" type="XPath">
                                        <a:tst expr="( ((node:fallback(.,1) &lt; 0) or (node:fallback(.,1) &gt; 2047)) and (node:fallback(../CanIdType, 'STANDARD') = 'STANDARD') )" 
                                                    true="CanIdValue is invalid. Please select proper CanIdValue according to CanIdType."/>
                                        <a:tst expr="( ((node:fallback(.,1) &lt; 0) or (node:fallback(.,1) &gt; 536870911)) and (node:fallback(../CanIdType, 'EXTENDED') = 'EXTENDED') )"
                                                    true="CanIdValue is invalid. Please select proper CanIdValue according to CanIdType."/>
                                        <a:tst expr="( ((node:fallback(.,1) &lt; 0) or (node:fallback(.,1) &gt; 536870911)) and (node:fallback(../CanIdType, 'MIXED') = 'MIXED') )" 
                                                    true="CanIdValue is invalid. Please select proper CanIdValue according to CanIdType."/>
                                    </a:da>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&gt;=0"/>
                                        <a:tst expr="&lt;=4294967295"/>
                                    </a:da>
                                </v:var>

                                <v:var name="CanMBPrio" type="INTEGER">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                MBCS[PRIO]: Local priority.<br>
                                                This 3-bit field is used when MCR[LPRIO_EN] is set and makes sense only for TX buffers.<br>
                                                If CTRL[LBUF] is set this field is not used.<br>
                                                These bits are not transmitted. They are appended to the regular ID to define the transmission priority.<br>
                                                <h1>Note</h1>Implementation specific Parameter. Not AutoSar Required.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can ID Bits Local Priority"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:84b6bd47-98f3-48df-940e-ecd1850089fc"/>
                                    <a:da name="EDITABLE" type="XPath" expr="../CanObjectType = 'TRANSMIT'"/>
                                    <a:da name="DEFAULT" value="0"/>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&gt;=0"/>
                                        <a:tst expr="&lt;=7"/>
                                    </a:da>
                                </v:var>

                                <!-- @implements DCAN02044 -->
                                <v:var name="CanObjectId" type="INTEGER">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN326_Conf. Holds the handle ID of HRH or HTH.<br>
                                                The value of this parameter is unique in a given CAN Driver, and it should start with 0 and continue without any gaps.<br>
                                                The HRH and HTH Ids are defined under two different name-spaces.<br>
                                                Example: HRH0-0, HRH1-1, HTH0-2, HTH1-3<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Object ID (MB Handle)"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                    <a:a name="UUID" value="ECUC:7ed539b0-94b8-42c3-9df9-ae96ce8cd9bf"/>
                                    <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index,'0')">
                                    </a:da>
                                    <a:da name="RANGE" type="XPath">
                                        <a:tst expr="(text:uniq(node:fallback(../../*/CanObjectId, text:split('1 2 3 4 5')), node:fallback(.,1)) and (node:fallback(.,1) &gt;= 0) and (node:fallback(.,1) &lt; num:i(count(node:fallback(node:current()/../../*, 1)))))
                                                    or ((../../../../../CanGeneral/CanMultiplexedTransmission='true') and (num:i(node:fallback(../CanObjectId,0)) = num:i(node:fallback(../../*/CanObjectId,0) )) and (../CanObjectType = 'TRANSMIT') )
                                                    "
                                            false="Wrong Object ID. If Multiplex transmission is OFF, Object IDs must be unique and in order. If multiplex transmission is ON, different HO can have the same Object ID but these must belong to the same controller, and must be of type TRANSMIT."/>
                                        <a:tst expr="( num:i(.) = num:i(0))
                                                    or  (num:i(node:fallback(../../*/CanObjectId,0) ) = num:i(node:fallback(../CanObjectId - 1,0)))"
                                            false="Wrong ObjectId. There must not be any gaps in the values of the CanObjectId of HardwareObjects."/>
                                    </a:da>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&gt;=0"/>
                                        <a:tst expr="&lt;=65535"/>
                                    </a:da>
                                </v:var>

                                <!-- @implements DCAN02046 -->
                                <v:var name="CanObjectType" type="ENUMERATION">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN327_Conf. Specifies if the HardwareObject is used as Transmit or as Receive object.<br>
                                                <h1>Note</h1>MBs configred as MIXED standard and RECEIVE type will be treated as EXTENDED.
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can MB Type"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:08567223-cf9a-46d6-860b-40bc10160518"/>
                                    <a:da name="DEFAULT" value="RECEIVE"/>
                                    <a:da name="RANGE">
                                        <a:v>RECEIVE</a:v>
                                        <a:v>TRANSMIT</a:v>
                                    </a:da>
                                </v:var>

                                <!-- @implements DCAN02021 -->
                                <v:ref name="CanControllerRef" type="REFERENCE">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                CAN322_Conf. Reference to CAN Controller to which the HOH is associated to.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Controller Reference"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="UUID" value="ECUC:ea88b350-ec0c-4ec6-81be-6feb79959689"/>
                                    <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Can/CanConfigSet/CanController"/>
                                </v:ref>

                                <!-- @implements DCAN02027 -->
                                <v:ref name="CanFilterMaskRef" type="REFERENCE">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Different CanHardwareObjects with different CanIdTypes (STANDARD, MIXED,  EXTENDED)
                                                can share the same CanFilterMask (i.e., the CanFilterMaskRef parameters of  these
                                                CanHardwareObjects reference the very same CanFilterMask container). This  shall be allowed and must be
                                                supported by the configuration generators.

                                                The CanFilterMaskRef is omitted for
                                                1) CanHardwareObjects with CanObjectType set to TRANSMIT
                                                2) CanHardwareObjects with CanObjectType set to RECEIVE if only a single Can ID
                                                   shall be received via this CanHardwareObjects(i.e. exact match with CanIdValue requested when CanHandleType is configured as FULL).
                                                CAN321_Conf. Reference to the filter mask that is used for hardware filtering together with the CAN_ID_VALUE.<br>
                                                This value is used as acceptance masks for ID filtering in RX MBs and the FIFO.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Can Filter Mask Reference"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ENABLE" type="XPath">
                                        <a:tst expr="../CanObjectType = 'RECEIVE' and ../CanHandleType = 'BASIC'"/>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="UUID" value="ECUC:f3303293-3cba-4b65-91eb-c56fccfe5a40"/>
                                    <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Can/CanConfigSet/CanController/CanFilterMask"/>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(num:i(node:ref(.)/CanFilterMaskValue) &gt;= 2048) and 
                                                     (node:current()/../CanIdType = 'STANDARD')" 
                                                true="CanFilterMaskValue should be equal or lower than 2047 (0x7FF -> 11 bits) for CanIdType as STANDARD."/>
                                    </a:da>
                                </v:ref>
                                 <!-- @implements DCAN02902 -->
                                <v:ref name="CanMainFunctionRWPeriodRef" 
                                       type="CHOICE-REFERENCE">
                                 <a:a name="DESC">
                                   <a:v>
                                      <![CDATA[EN:<html>
                                          CAN438_Conf.Reference to CAN Controller to which the HOH is associated to.<br>
                                        </html>]]>
                                    </a:v>
                                  </a:a>
                                   <a:a name="LABEL" value="Can MainFunction RW Period Reference"/>
                                   <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                   </a:a>
                                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                  <a:a name="OPTIONAL" value="true"/>
                                  <a:a name="UUID" 
                                       value="ECUC:e69d45af-b667-4a3a-b41a-e9551b85bf58"/>
                                  <a:da name="REF">
                                   <a:v>ASPathDataOfSchema:/AUTOSAR/EcucDefs/Can/CanGeneral/CanMainFunctionRWPeriods</a:v>
                                  </a:da>
                                    <a:da name="INVALID" type="XPath">
                                    <a:tst expr="(((../CanObjectType = 'TRANSMIT') and (count(node:ref(.)/CanMainFunctionWritePeriod/*) = 0)) or ((../CanObjectType = 'RECEIVE') and (count(node:ref(.)/CanMainFunctionReadPeriod/*) = 0)))" 
                                            true="CanMainFunctionWritePeriod should have one child when CanObjectType is TRANSMIT and CanMainFunctionReadPeriod should have one child when CanObjectType is RECEIVE."/>
                                    <a:tst expr="(../CanObjectType = 'RECEIVE') and (node:value((node:ref(../CanControllerRef)/CanRxProcessing)) = 'Polling') and (node:empty(.))" 
                                            true="CanMainFunctionRWPeriodRef should refer to CanMainFunctionReadPeriod when CanObjectType is RECEIVE and CanRxProcessing is configured as POLLING."/>
                                    <a:tst expr="(../CanObjectType = 'TRANSMIT') and (node:value((node:ref(../CanControllerRef)/CanTxProcessing)) = 'Polling') and (node:empty(.))" 
                                            true="CanMainFunctionRWPeriodRef should refer to CanMainFunctionWritePeriod when CanObjectType is TRANSMIT and CanTxProcessing is configured as POLLING"/>
                                    </a:da>
                                </v:ref>                        
                                <v:lst name="CanTTHardwareObjectTrigger" type="MAP">
                                    <a:da name="EDITABLE" value="false" />
                                    <a:da name="ENABLE" value="false" /> 
                                    <v:ctr name="CanTTHardwareObjectTrigger" 
                                           type="IDENTIFIABLE">
                                      <a:a name="DESC" 
                                           value="EN: This container is only included and valid if TTCAN SWS is used and TTCAN is enabled."/>
                                      <a:a name="UUID" 
                                           value="ECUC:e4ab5574-20a9-4d2d-86ad-d7e23b583932"/>
                                      <v:var name="CanTTHardwareObjectBaseCycle" 
                                             type="INTEGER">
                                        <a:a name="DESC" 
                                             value="EN: Defines the cycle_offset."/>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                             type="IMPLEMENTATIONCONFIGCLASS">
                                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
