                          </a:a>
                          <a:a name="ORIGIN" value="Freescale"/>
<!--                      <a:a name="LOWER-MULTIPLICITY" value="1"/>
                          <a:a name="UPPER-MULTIPLICITY" value="1"/>-->
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:dcc46433-9de4-4883-9bd4-84274be6e67f"/>
                          <a:da name="DEFAULT" value="BL_13"/>
                          <a:da name="RANGE">
                              <a:v>BL_10</a:v>
                              <a:v>BL_11</a:v>
                              <a:v>BL_12</a:v>
