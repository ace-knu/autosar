                    <!-- @implements DLIN01003 -->
                    <v:ctr name="LinChannel" type="IDENTIFIABLE">
                      <a:a name="DESC">
                          <a:v>
                              <![CDATA[EN:
                                  <html>
                                  This container contains the configuration (parameters) of the LIN Controller(s).
                                  <h1>Note:</h1>&quot;User should use unique names for naming the LIN channels across different LinGlobalConfig Sets.&quot;
                                  </html>
                              ]]>
                          </a:v>
                      </a:a>
                      <a:a name="UUID" 
                           value="ECUC:fdac1ad9-1217-44a8-bc3f-341635a20162"/>
<!--                  <a:a name="LOWER-MULTIPLICITY" value="1"/>
                      <a:a name="UPPER-MULTIPLICITY" value="1"/> -->
                      <!-- @implements DLIN01006 -->
                      <v:var name="LinChannelId" type="INTEGER">
                        <a:a name="DESC">
                            <a:v>
                                <![CDATA[EN:
                                    <html>
                                    Identifies the LIN channel. Replaces LIN_CHANNEL_INDEX_NAME from the LIN SWS.
                                    </html>
                                ]]>
                            </a:v>
                        </a:a> 
                        <a:a name="LABEL" value="Lin Channel ID"/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
<!--                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                        <a:a name="UPPER-MULTIPLICITY" value="1"/>-->
                        <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                        <a:a name="UUID" 
                             value="ECUC:aac952ef-7cb7-4b94-acc5-b6c4ca835e7a"/>
                        <a:a name="EDITABLE" value="true"/>
                        <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index, num:i(1))"/>
                        <a:da name="INVALID" type="Range">
                            <a:tst expr="&gt;=0"/>
                            <a:tst expr="&lt;=10"/>
                        </a:da>
                        <a:da name="RANGE" type="XPath">
                            <a:tst expr="(. &gt;= 0) and (.&lt; num:i(count(node:current()/../../*)))"
                                    false="Value out of range: must be in range 0 to N-1 (N is number of configured channels). Use the Calc button to calculate correct default value"/>
                            <a:tst expr="text:uniq(../../*/LinChannelId, .)"
                                    false="Duplicated value, LinChannelId must be unique across all Lin channels. Use the Calc button to calculate correct default value."/>
                            <a:tst expr="(. &gt;=10)"
                                    true="Maximum lin channels supported by this plugin is 10 only."/>                
                        </a:da>
                      </v:var>
                      <!-- @implements DLIN01004 -->
                      <v:var name="LinChannelBaudRate" type="INTEGER">
                        <a:a name="DESC">
                         <a:v>
                             <![CDATA[EN:
                                 <html>
                                   <h1>LinChannelBaudRate</h1>
                                   <p>Autosar Requirements: LIN180_Conf</p>
                                   <p>Specifies the baud rate of the LIN channel in 'bps'. Valid range: 1000..20000.</p>
                                 </html>
                        ]]>
                         </a:v>
                        </a:a>
                        <a:a name="LABEL" value="Lin Channel BaudRate (bps)"/>
